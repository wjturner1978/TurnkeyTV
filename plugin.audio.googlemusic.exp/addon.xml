<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.audio.googlemusic.exp"
       name="Google Music EXP"
       version="1.7.1"
       provider-name="ForeverGuest">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.decorator" version="3.4.0"/>
    <import addon="script.module.requests" version="1.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Experimental Google Music audio plugin</summary>
    <description lang="en">Plugin that lets you play music from Google Music.</description>
  </extension>
</addon>
